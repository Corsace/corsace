{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "Build",
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "strict": true,
        "noImplicitAny": false,
        "types": [
            "@types/node",
            "@nuxt/types",
            "nuxt-i18n"
        ]
    }
}
