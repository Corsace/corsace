{
   "name": "corsace",
   "version": "1.0.0",
   "description": "Everything Corsace",
   "main": "index.ts",
   "dependencies": {
      "@koa/router": "^10.0.0",
      "@nuxt/typescript-runtime": "^2.0.0",
      "@nuxtjs/axios": "^5.12.5",
      "@nuxtjs/proxy": "^2.1.0",
      "discord.js": "^12.5.1",
      "googleapis": "^88.2.0",
      "koa": "^2.13.1",
      "koa-bodyparser": "^4.3.0",
      "koa-cash": "^4.0.5",
      "koa-mount": "^4.0.0",
      "koa-passport": "^4.1.3",
      "koa-session": "^6.1.0",
      "lodash": "^4.17.20",
      "lru-cache": "^4.1.5",
      "mysql": "^2.18.1",
      "node-config-ts": "^3.1.0",
      "nodesu": "https://github.com/ThePooN/nodesu/archive/502d65b9f84dd2fbe0a24f4eeaf9359305f8bcc3.tar.gz",
      "nuxt": "^2.14.12",
      "nuxt-i18n": "^6.17.0",
      "passport": "^0.4.1",
      "passport-discord": "^0.1.4",
      "passport-oauth2": "^1.5.0",
      "reflect-metadata": "^0.1.10",
      "stream-to-string": "^1.2.0",
      "ts-node": "^9.1.1",
      "typeorm": "^0.2.29",
      "typescript": "^4.1.3",
      "vue-class-component": "^7.2.6",
      "vue-property-decorator": "^9.1.2",
      "vuex-class": "^0.3.2"
   },
   "devDependencies": {
      "@nuxt/types": "^2.14.12",
      "@nuxt/typescript-build": "^2.0.3",
      "@types/async": "^3.2.5",
      "@types/koa": "^2.11.6",
      "@types/koa__router": "^8.0.3",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-cash": "^4.0.0",
      "@types/koa-mount": "^4.0.0",
      "@types/koa-passport": "^4.0.2",
      "@types/koa-session": "^5.10.3",
      "@types/lodash": "^4.14.167",
      "@types/lru-cache": "^5.1.0",
      "@types/memoizee": "^0.4.5",
      "@types/node": "^14.14.20",
      "@types/passport-discord": "^0.1.3",
      "@types/validator": "^13.1.2",
      "@typescript-eslint/eslint-plugin": "^4.12.0",
      "@typescript-eslint/parser": "^4.12.0",
      "async": "^3.2.0",
      "concurrently": "^5.3.0",
      "cross-env": "^7.0.3",
      "eslint": "^7.17.0",
      "eslint-plugin-vue": "^7.4.1",
      "memoizee": "^0.4.15",
      "sass": "^1.32.2",
      "sass-loader": "^10.1.0",
      "ts-node-dev": "^1.1.8"
   },
   "scripts": {
      "build:ayim": "cd ./AYIM && nuxt-ts build",
      "build:mca": "cd ./MCA && nuxt-ts build",
      "build:main": "cd ./Main && nuxt-ts build",
      "build:open": "cd ./Open && nuxt-ts build",
      "build:api": "cross-env NODE_ENV=production tsc --project Server/tsconfig.json",
      "build:discord-bot": "cross-env NODE_ENV=production tsc --project DiscordBot/tsconfig.json",

      "ayim": "cd ./AYIM && nuxt-ts start",
      "mca": "cd ./MCA && nuxt-ts start",
      "main": "cd ./Main && nuxt-ts start",
      "open": "cd ./Open && nuxt-ts start",
      "api": "cd Server && node dist/Server/index.js",
      "discord-bot": "cd DiscordBot && node dist/DiscordBot/index.js",

      "ayim-dev": "cd ./AYIM && nuxt-ts",
      "mca-dev": "cd ./MCA && nuxt-ts",
      "main-dev": "cd ./Main && nuxt-ts",
      "open-dev": "cd ./Open && nuxt-ts",
      "api-dev": "cross-env NODE_ENV=development ts-node-dev --respawn Server/index.ts",
      "discord-bot-dev": "cross-env NODE_ENV=development ts-node-dev --respawn DiscordBot/index.ts",

      "dev": "concurrently -n MCA,AYIM,MAIN,OPEN,API -c bgMagenta.bold,bgBlue.bold,black.bgWhite.bold,bgRed.bold,black.bgYellow.bold \"npm:mca-dev\" \"npm:ayim-dev\" \"npm:main-dev\" \"npm:open-dev\" \"npm:api-dev\"",
      "dev-client": "concurrently -n MCA,AYIM,MAIN,OPEN -c bgMagenta.bold,bgBlue.bold,black.bgWhite.bold,bgRed.bold \"npm:mca-dev\" \"npm:ayim-dev\" \"npm:main-dev\" \"npm:open-dev\"",

      "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
      "lintfix": "eslint --fix --ext .ts,.vue --ignore-path .gitignore .",

      "updateLanguages": "ts-node ./Server/scripts/updateLanguages.ts",
      "fetchMaps": "ts-node ./Server/scripts/fetchYearMaps.ts",

      "database": "docker-compose up -d database",
      "mariadb": "docker-compose exec database mariadb -u corsace -p",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Corsace/Corsace.git"
   },
   "keywords": [
      "osu",
      "corsace",
      "rhythm",
      "game"
   ],
   "author": "VINXIS",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/Corsace/Corsace/issues"
   },
   "homepage": "https://github.com/Corsace/Corsace#readme"
}
